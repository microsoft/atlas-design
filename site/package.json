{
	"name": "@microsoft/atlas-site",
	"version": "0.65.0",
	"license": "MIT",
	"private": true,
	"engines": {
		"node": "24.11.1",
		"npm": "11.6.2"
	},
	"targets": {
		"default": {
			"includeNodeModules": true
		}
	},
	"scripts": {
		"prebuild": "node toc.js",
		"prestart": "node toc.js",
		"start": "cross-env PARCEL_WORKERS=0 parcel \"./src/**/*.md\" --port 1111",
		"build": "cross-env PARCEL_WORKERS=0 parcel build \"./src/**/*.md\"",
		"toc": "node toc.js",
		"type-check": "tsc --noEmit",
		"prelint": "npm run type-check",
		"lint": "eslint . --ext .ts",
		"lint-fix": "eslint . --ext .ts --fix"
	},
	"devDependencies": {
		"cross-env": "^7.0.3",
		"eslint": "^8.43.0",
		"@typescript-eslint/eslint-plugin": "^5.60.1",
		"@typescript-eslint/parser": "^5.60.1",
		"eslint-plugin-import": "^2.27.5",
		"@microsoft/atlas-css": "4.1.0",
		"eslint-config-prettier": "^8.8.0",
		"parcel": "2.16.3",
		"lightningcss": "1.30.2",
		"@parcel/transformer-sass": "2.16.3",
		"@microsoft/parcel-transformer-markdown-html": "2.7.2",
		"fs-extra": "^11.1.1",
		"front-matter": "^4.0.2",
		"typescript": "^5.1.5",
		"@microsoft/atlas-js": "^1.15.0"
	}
}
